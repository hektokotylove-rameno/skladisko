<h1>Chemicals</h1>
<table class="table table-striped">
  <tr>
    <th>Name</th>
    <th>Total amount</th>
    <th>Unit</th>
    <th>Group</th>
    <th>Note</th>
    <th>Actions</th>
  </tr>
 
<% @chemicals.each do |chemical| %>
  <tr>
    <td><%= chemical.name %></td>
    <td><%= chemical.total_amount %></td>
    <td><%= chemical.unit %></td>
    <td><%= chemical.group %></td>
    <td><%= chemical.note %></td>
    <td>
      <div class="btn-group">
          <%= link_to 'Show', chemical, :class => "btn btn-default" %>
          <%= link_to 'Edit', edit_chemical_path(chemical), :class => "btn btn-default" %>
      </div>
    </td>
    <!--<td><%= link_to 'Show', chemical %></td>
    <td><%= link_to 'Edit', edit_chemical_path(chemical) %></td>-->
  </tr>
<% end %>
</table>

<%= link_to 'Create new', new_chemical_path, class: "btn btn-primary" %>
