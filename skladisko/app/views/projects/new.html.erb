<h1>Create Project</h1>
<div class="col-xs-4">  
<%= form_for :project, url: projects_path do |p| %>
  <p>
    <%= p.label :name %><br>
    <%= p.text_field :name, class: "form-control", placeholder: "Name"  %>
  </p>
  <p> <%= p.submit class: "btn btn-primary"  %></p>  
<% end %>

<%= link_to 'Back', projects_path %>
</div>