<h1>Projects</h1>
<table class="table table-striped">
  <tr>
    <th>ID</th>
    <th>Name</th>
  </tr>
  <% @projects.each do |project| %>
    <tr>
      <td><%= project.id %></td>
      <td><%= project.name %></td>
    </tr>
  <% end %>
</table>
<%= link_to 'Create New', new_project_path, class: "btn btn-primary" %>

